import (
	"database/sql"
	"encoding/json"
	"log"
	"net/http"

	_ "github.com/mattn/go-sqlite3"
)

type Dessert struct {
	ID          int    `json:"id"`
	Name        string `json:"name"`
	ImageURL    string `json:"image_url"`
	Ingredients string `json:"ingredients"`
	BakeTime    string `json:"bake_time"`
	RecipeLink  string `json:"recipe_link"`
}